            </div>
        </div>

        <div class="container">
            <div class="row karl-new-arrivals" style="position: relative; height: 1094.03px;">

                <!-- Single gallery Item Start -->
                @{ int i = 0;}
                @foreach (var item in Model)
                {
                    
                        if (i < 6)
                        {
                        <div class="col-12 col-sm-6 col-md-4 single_gallery_item @item.TypeId wow fadeInUpBig" data-wow-delay="0.2s" style="visibility: hidden; animation-delay: 0.2s; animation-name: none; position: absolute; left: 0%; top: 0px;">
                            <!-- Product Image -->
                            <div class="product-img">
                                <img src="~/images/upload/@item.Img" alt="">
                                <div class="product-quicview">
                                    <a href="#" data-toggle="modal" data-target="#@item.Id"><i class="ti-eye"></i></a>
                                </div>
                            </div>
                            <!-- Product Description -->
                            <div class="product-description">
                                <h4 class="product-price">@{var value = String.Format("{0,-5:N0}đ", item.Price); }@value</h4>
                                <p>@item.Name</p>
                                <form asp-action="AddToShoppingCart"
                                      asp-controller="ShoppingCart"
                                      asp-route-cosmeticsId="@item.Id"
                                      method="post">
                                    <button class="btn btn-no-effect add-to-cart-btn">Thêm vào giỏ hàng</button>
                                </form>
                            </div>
                        </div>
                        }i++;
                    }
                
            </div>
        </div>
    </section>
