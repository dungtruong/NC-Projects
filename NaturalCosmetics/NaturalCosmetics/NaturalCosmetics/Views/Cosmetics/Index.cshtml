                <button class="btn active" data-filter="*">ALL</button>
                <button class="btn" data-filter=".1">Mỹ Phẩm</button>
                <button class="btn" data-filter=".2">Dưỡng Da Mặt</button>
                <button class="btn" data-filter=".3">Dưỡng Toàn Thân</button>
                <button class="btn" data-filter=".4">Dưỡng Tóc</button>
