    <!-- ****** Quick View Modal Area Start ****** -->
    @foreach (var item in Model)
    {
        <div class="modal fade" id="@item.Id" tabindex="-1" role="dialog" aria-labelledby="quickview" aria-hidden="true">
            <div class="modal-dialog modal-lg modal-dialog-centered" role="document">
                <div class="modal-content">
                    <button type="button" class="close btn" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">×</span>
                    </button>

                    <div class="modal-body">
                        <div class="quickview_body">
                            <div class="container">
                                <div class="row">
                                    <div class="col-12 col-lg-5">
                                        <div class="quickview_pro_img">
                                            <img src="~/images/upload/@item.Img" alt="">
                                        </div>
                                    </div>
                                    <div class="col-12 col-lg-7">
                                        <div class="quickview_pro_des">
                                            <h4 class="title">@item.Name</h4>
                                            <div class="top_seller_product_rating mb-15">
                                                <i class="fa fa-star" aria-hidden="true"></i>
                                                <i class="fa fa-star" aria-hidden="true"></i>
                                                <i class="fa fa-star" aria-hidden="true"></i>
                                                <i class="fa fa-star" aria-hidden="true"></i>
                                                <i class="fa fa-star" aria-hidden="true"></i>
                                            </div>
                                            <h5 class="price">@{var value = String.Format("{0,-5:N0}đ", item.Price); }@value </h5>
                                            <p>@item.Detail</p>
                                            <a asp-area="" asp-controller="Cosmetics" asp-action="ChiTietSP" asp-route-id="@item.Id">Xem chi tiết sản phẩm</a>
                                        </div>
                                        <!-- Add to Cart Form -->
                                        
                                            
                                            <form asp-action="AddToShoppingCart"
                                                  asp-controller="ShoppingCart"
                                                  asp-route-cosmeticsId="@item.Id"
                                                  method="post">
                                                <button class="btn btn-no-effect add-to-cart">Thêm vào giỏ hàng</button>
                                            </form>                                            
                                            
                                        

                                        
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
    <!-- ****** Quick View Modal Area End ****** -->
